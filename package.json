{
  "name": "aws-copilot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bash ./scripts/run-backend.sh dev\" \"pnpm --dir frontend dev\"",
    "build": "pnpm --dir frontend build",
    "start": "bash ./scripts/run-backend.sh start",
    "frontend:dev": "pnpm --dir frontend dev",
    "frontend:build": "pnpm --dir frontend build",
    "frontend:lint": "pnpm --dir frontend lint",
    "frontend:preview": "pnpm --dir frontend preview",
    "frontend:test": "pnpm --dir frontend test",
    "backend:dev": "bash ./scripts/run-backend.sh dev",
    "backend:start": "bash ./scripts/run-backend.sh start",
    "dev:up": "bash ./scripts/dev-up.sh"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
